  "description": "StudyHi - Welcome to Learning",
  "homepage": "./",
  "private": true,
  "scripts": {
    "build": "next build",
    "dev": "node server.js",
    "lint": "next lint",
    "start": "node server.js",
    "start:production": "set NODE_ENV=production && next start",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:install": "playwright install",
    "test:runner": "node scripts/test-runner.js",
    "test:performance": "node scripts/performance-test.js",
    "test:accessibility": "node scripts/accessibility-test.js",
    "test:coverage-report": "node scripts/coverage-report.js",
    "test:all": "npm run test:ci && npm run test:e2e",
    "test:quick": "jest --passWithNoTests --silent",
    "test:debug": "jest --detectOpenHandles --forceExit",
    "build:analyze": "ANALYZE=true npm run build",
    "build:production": "set NODE_ENV=production && npm run build",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "clean": "if exist .next rmdir /s /q .next && if exist out rmdir /s /q out && if exist coverage rmdir /s /q coverage && if exist test-results rmdir /s /q test-results && if exist playwright-report rmdir /s /q playwright-report && if exist dist rmdir /s /q dist",
    "prebuild": "npm run clean",
    "postbuild": "echo 'Build completed successfully'",
    "production:build": "node scripts/production-build.js",
    "deploy:production": "node scripts/deploy-production.js",
    "test:auth": "node scripts/test-auth.js"
  },
